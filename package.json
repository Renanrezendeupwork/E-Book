  "scripts": {
    "start": "export REACT_APP_BUILD_VERSION=`git rev-parse HEAD` && react-scripts start",
    "docker": "cd ../flangoo-back-end && docker-compose up -d server; cd ../flangoo_php && docker-compose up -d server_admin; cd ../Flangoo-Front-End && docker-compose up frontend",
    "down": "cd ../flangoo-back-end && docker-compose down; cd ../flangoo_php && docker-compose down; cd ../Flangoo-Front-End && docker-compose down",
    "start_w": "react-scripts start",
    "build": "export REACT_APP_BUILD_VERSION=`git rev-parse HEAD` && react-scripts --openssl-legacy-provider build ",
    "build:staging": "env-cmd -f .env.staging npm run-script build",
    "build:cicd": "env-cmd -f .env.cicd npm run-script build",
    "test": "cypress run",
    "test_live": "cypress run --config-file cypress/config/live/cypress.config.ts",
    "test_stage": "cypress run --config-file cypress/config/stage/cypress.config.ts",
    "test_run": "cypress open",
    "test_run_stage": "cypress open --config-file cypress/config/stage/cypress.config.ts",
    "test_run_live": "cypress open --config-file cypress/config/live/cypress.config.ts",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/scss/:src/css",
    "pull": "git pull; npm run sass_update",
    "sass_update": "sass --update src/scss/:src/css",
    "update_fontawesome": "npm install --save-dev @fortawesome/fontawesome-free; cp node_modules/@fortawesome/fontawesome-free/css/all.min.css src/css/font-awesome.css; cp -r node_modules/@fortawesome/fontawesome-free/webfonts  src/css/fonts/",
    "update_fontawesome_pro": "npm install --save-dev @fortawesome/fontawesome-pro; cp node_modules/@fortawesome/fontawesome-pro/css/all.min.css src/css/font-awesome-pro.css; cp -r node_modules/@fortawesome/fontawesome-pro/webfonts  src/css/fonts/",
    "update_bootstrap": "npm install bootstrap --save; cp node_modules/bootstrap/dist/js/bootstrap.min.js js/bootstrap.min.js; cp -r node_modules/bootstrap/scss  src/scss/bootstrap/; sass --update scss:css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-pro": "^5.15.4",
    "@sentry/webpack-plugin": "^1.16.0",
    "@types/chance": "^1.1.3",
    "@types/dompurify": "^2.2.3",
    "@types/luxon": "^1.27.0",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-google-recaptcha": "^2.1.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-keyboard-event-handler": "^1.5.2",
    "@types/react-router-hash-link": "^2.4.6",
    "cypress": "^10.3.0",
    "env-cmd": "^10.1.0",
    "gulp": "^4.0.2"
  }
}
